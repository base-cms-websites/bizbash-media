{
  "<global-content-list-flow>": {
    "template": "./content-list.marko",
    "<header>": {},
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@class": "string",
    "@nodes": "array",
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": false
    },
    "@inner-justified": {
      "type": "boolean",
      "default-value": true
    },
    "@image-width": {
      "type": "number",
      "default-value": 126
    },
    "@image-ar": {
      "type": "string",
      "default-value": "3:2"
    },
    "@image-position": {
      "type": "string",
      "default-value": "left"
    },
    "@bottom-leaderboard": "object"
  },
  "<global-content-hero-flow>": {
    "template": "./content-hero.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@nodes": "array"
  },
  "<global-content-load-more-flow>": {
    "template": "./content-load-more.marko",
    "@nodes": "array",
    "@aliases": "array"
  },
  "<global-content-card-deck-flow>": {
    "template": "./content-card-deck.marko",
    "<native-x>": {
      "@index": "number",
      "@name": "string",
      "@aliases": "array"
    },
    "@col": {
      "type": "number",
      "default-value": 3
    },
    "@nodes": "array",
    "@aliases": "array",
    "@ad-index": "number",
    "@ad-name": "string",
    "@ad-position": "string",
    "@display-image": {
      "type": "boolean",
      "default-value": true
    },
    "@with-teaser": {
      "type": "boolean",
      "default-value": false
    },
    "@bottom-leaderboard": "object"
  },
  "<global-magazine-issue-archive-flow>": {
    "template": "./magazine-issue-archive.marko",
    "@nodes": "array",
    "@flush": {
      "type": "boolean",
      "default-value": false
    },
    "@with-title": {
      "type": "boolean",
      "default-value": true
    },
    "@modifiers": "array"
  },
  "<global-magazine-latest-issue-flow>": {
    "template": "./magazine-latest-issue.marko",
    "<header>": {},
    "@issue-node": "object"
  }
}
